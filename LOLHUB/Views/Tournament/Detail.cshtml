@model IList<LOLHUB.Models.Player>
@{
    ViewData["Title"] = "Index";
}

<h1>DetailPage of Tournament with id = @ViewBag.TournamentId </h1>
<h3>List of players who join this tournament: </h3>


<table class="table table-hover table-striped">
    <tr>
        <th>Player ID</th>
        <th>Player Name</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
        </tr>
    }
    </table>
